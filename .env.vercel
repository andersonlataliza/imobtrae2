# Configuração de Variáveis de Ambiente para Vercel
# Copie estas variáveis para o painel da Vercel

# === SUPABASE CONFIGURATION ===
# URL do seu projeto Supabase
VITE_SUPABASE_URL=https://your-project.supabase.co

# Chave anônima do Supabase (pública)
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Chave de service role do Supabase (PRIVADA - apenas para deploy)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# === APPLICATION SETTINGS ===
# Nome da aplicação
VITE_APP_NAME=IMOBTRAE

# URL da aplicação (será definida automaticamente pela Vercel)
VITE_APP_URL=https://your-app.vercel.app

# Descrição da aplicação
VITE_APP_DESCRIPTION=Plataforma completa para gestão imobiliária

# === DEPLOYMENT SETTINGS ===
# Usar seeders completos (true) ou básicos (false)
USE_COMPLETE_SEEDERS=true

# Ambiente de execução
NODE_ENV=production

# === OPTIONAL SETTINGS ===
# Email de contato
VITE_CONTACT_EMAIL=contato@imobtrae.com

# Telefone de contato
VITE_CONTACT_PHONE=(11) 99999-9999

# Endereço da empresa
VITE_COMPANY_ADDRESS=São Paulo, SP

# === SOCIAL MEDIA (OPCIONAL) ===
VITE_FACEBOOK_URL=https://facebook.com/imobtrae
VITE_INSTAGRAM_URL=https://instagram.com/imobtrae
VITE_LINKEDIN_URL=https://linkedin.com/company/imobtrae
VITE_WHATSAPP_NUMBER=5511999999999

# === ANALYTICS (OPCIONAL) ===
# Google Analytics ID
VITE_GA_TRACKING_ID=G-XXXXXXXXXX

# === MAPS (OPCIONAL) ===
# Google Maps API Key
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# === SECURITY ===
# JWT Secret (gerado automaticamente pelo Supabase)
# Não é necessário definir manualmente

# === FILE UPLOAD LIMITS ===
VITE_MAX_FILE_SIZE=5242880
VITE_MAX_FILES_PER_PROPERTY=10
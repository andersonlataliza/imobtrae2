# Supabase Configuration
# Copy this file to .env.local and fill in your actual values

# Supabase Project URL
# Get this from your Supabase project dashboard -> Settings -> API
VITE_SUPABASE_URL=https://your-project-ref.supabase.co

# Supabase Anonymous Key
# Get this from your Supabase project dashboard -> Settings -> API
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Supabase Service Role Key (Optional - for admin operations)
# ⚠️ NEVER expose this key in your frontend code!
# Only use in server-side operations or development
VITE_SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Application Configuration
VITE_APP_NAME=IMOBTRAE
VITE_APP_URL=http://localhost:5173
VITE_APP_DESCRIPTION="Imobiliária Elite - Encontre o imóvel perfeito"

# Development Configuration
VITE_DEV_MODE=true
VITE_ENABLE_ANALYTICS=false

# Email Configuration (Optional - for contact forms)
VITE_CONTACT_EMAIL=contato@imobiliaria.com
VITE_SUPPORT_EMAIL=suporte@imobiliaria.com

# Social Media Links (Optional)
VITE_FACEBOOK_URL=https://facebook.com/imobiliaria
VITE_INSTAGRAM_URL=https://instagram.com/imobiliaria
VITE_LINKEDIN_URL=https://linkedin.com/company/imobiliaria

# Google Analytics (Optional)
VITE_GA_TRACKING_ID=G-XXXXXXXXXX

# Map Configuration (Optional - for property locations)
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# File Upload Configuration
VITE_MAX_FILE_SIZE=5242880
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/webp

# Security Configuration
VITE_ENABLE_RLS=true
VITE_SESSION_TIMEOUT=3600